{
  "expo": {
    "name": "Moments",
    "description": "Capture and explore the amazing moments, memories, people, places, and activities in your life.",
    "slug": "moments",
    "scheme": "moments",
    "privacy": "public",
    "githubUrl": "https://github.com/davidhartsough/moments-mobile-app",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android"],
    "version": "0.1.1",
    "orientation": "portrait",
    "primaryColor": "#1471eb",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1471eb"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.davidhartsough.moments",
      "buildNumber": "0.1.0",
      "icon": "./assets/images/ios-icon.png",
      "supportsTablet": true,
      "config": {
        "googleSignIn": {
          "reservedClientId": ""
        }
      }
    },
    "android": {
      "package": "com.davidhartsough.moments",
      "versionCode": 2,
      "icon": "./assets/images/android-icon.png",
      "adaptiveIcon": {
        "backgroundColor": "#1471eb",
        "foregroundImage": "./assets/images/foreground.png"
      },
      "permissions": [],
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.davidhartsough.moments",
      "config": {
        "googleSignIn": {
          "apiKey": "",
          "certificateHash": ""
        }
      }
    }
  }
}
